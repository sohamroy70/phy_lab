def abc():

    s = "PytHON programming"

    print("*".join(s))

    print(s.upper())

    print(s.isupper())

    print(s.islower())

    print(s.replace("Python","C"))

    print(s[0:4])

    print(len(s))

    print(s[6])

    print(s.split(' '))

    print(s.capitalize())

    print("CMR" + "University")

    print('"CMR"' +'"University"')

    print("\"CMR\" \"University\"")


abc()
